<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="MySQLTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
            <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1/logistics?useUnicode=true&amp;characterEncoding=utf-8&amp;zeroDateTimeBehavior=convertToNull"
                        userId="root" password="root">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--
        生成系统类时配置：com.hna.logistics.common.system.entity
        生成基本业务类时配置：com.hna.logistics.data.biz.basic.entity
        生成业务类时配置：com.hna.logistics.data.biz.entity
        生成基本供应商类时配置：com.hna.logistics.data.sup.basic.entity
        生成业务类时配置：
        -->
        <javaModelGenerator targetPackage="cn.mysic.common.model.pay"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--
        生成系统类时配置：com.hna.logistics.common.system
        生成基本业务类时配置：com.hna.logistics.data.biz.basic.mapper
        生成基本供应商类时配置：com.hna.logistics.data.sup.basic.mapper
        生成业务类时配置：
        -->
        <sqlMapGenerator targetPackage="cn.mysic.common.model.pay"
                         targetProject="src/main/resources/mapper">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <table schema="logistics" tableName="pay_citic_internal_request" domainObjectName="PayCTITCInternalRequest"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false" enableDeleteByPrimaryKey="true"
               enableUpdateByPrimaryKey="true">
            <property name="rootClass" value="com.hna.logistics.common.core.entity.EntityBase"/>
            <property name="useActualColumnNames" value="true"/>
        </table>

    </context>
</generatorConfiguration>